---
import Contact from "../components/Contact.astro";
import Layout from "../layouts/Layout.astro";
import Banner from "../components/Banner.astro";
---

<Layout title="BYU_SuperApp">
  <main class="bg-blue-600">
    <section
      class="h-[80vh] max-[800px]:h-fit flex justify-center items-center"
    >
      <div
        class="grid grid-cols-[1fr_1.2fr] gap-10 px-8 max-[800px]:grid-cols-1 h-[80vh] max-[700px]:items-center max-[700px]:px-0 scale-75"
      >
        <div
          class={`bg-[#cce8fa] py- max-[770px]:py-10 px-20 rounded-3xl text-white flex justify-center items-center max-[700px]:hidden`}
        >
          <img
            class="w-[10vw] max-[800px]:h-[30vh] object-contain"
            src="/image-040ade60-a8fb-46a8-b463-4d07c00a8df8.webp"
            alt=""
          />
          <img
            class="w-[10vw] max-[800px]:h-[30vh] object-contain"
            src="/image-040ade60-a8fb-46a8-b463-4d07c00a8df8.webp"
            alt=""
          />
          <img
            class="w-[10vw] max-[800px]:h-[30vh] object-contain"
            src="/image-040ade60-a8fb-46a8-b463-4d07c00a8df8.webp"
            alt=""
          />
        </div>
        <div
          class="bg-white flex py-24 max-[770px]:py-10 px-20 rounded-3xl relative max-[800px]:w-[55vw] max-[800px]:h-[40vh] max-[700px]:h-4/5 max-[700px]:w-[90vw] max-[700px]:rounded-2xl"
        >
          <div
            class="absolute top-5 max-[1000px]:top-3 max-[1000px]:right-3 right-5 pt-16 max-[1000px]:pt-14 pb-5 max-[1000px]:pb-2 pl-5 pr-40 max-[1000px]:pr-24 bg-blue-600 rounded-2xl max-[1000px]:rounded-xl"
          >
            <a
              href="/"
              class="text-blue-100 border-b-2 font-CoFoBold text-lg max-[1000px]:text-base max-[800px]:text-sm"
              >↖ regresar al Home</a
            >
          </div>

          <h2
            class="text-7xl max-xl:text-6xl text-blue-600 absolute left-0 bottom-0 p-10 font-CoFoBold max-[1000px]:text-5xl max-[700px]:text-4xl max-[700px]:p-5 max-[700px]:pr-0"
          >
            Algunos de nuestros modelos <br />
            <span
              class="text-2xl max-xl:text-xl max-[1000px]:text-base pt-10 text-gray-400"
              >ARMÁ EL TUYO COMO QUIERAS</span
            >
          </h2>
        </div>
      </div>
    </section>
    <Banner url="\superapp-bannner.webp" />
    <section class="grid place-content-center w-full pb-28">
      <div class="flex justify-center items-center flex-col gap-16 w-full view">
        <div
          class="flex justify-center items-center gap-16 flex-row w-4/5 max-[1280px]:w-full max-[1280px]:flex-col views"
        >
          <video
            class="w-2/4 max-[800px]:w-[90vw] h-full object-cover rounded-3xl"
            autoplay
            muted
            loop
          >
            <source src="/daraval-video.mp4" type="video/mp4" />
          </video>
          <div
            class="relative bg-white w-full rounded-3xl h-full px-10 max-[450px]:px-5 py-5 flex justify-center items-center flex-col gap-5 max-[1280px]:w-2/4 max-[800px]:w-[90vw]"
          >
            <div class="flex justify-center items-start flex-col gap-5">
              <h2 class="text-5xl text-blue-600 font-CoFoMedium">
                Modelo Insurtech
              </h2>
              <ul class="flex justify-center items-start flex-col gap-3">
                <li class="text-gray-600 text-lg">
                  <span class="text-blue-600 font-semibold">⭢</span>
                  <span class="italic font-CoFoMedium">Pago</span> de pólizas
                </li>
                <li class="text-gray-600 text-lg">
                  <span class="text-blue-600 font-semibold">⭢</span>
                  <span class="italic font-CoFoMedium">Acceso</span> al ecosistema
                  de mundo urbana
                </li>
                <li class="text-gray-600 text-lg">
                  <span class="text-blue-600 font-semibold">⭢</span>
                  <span class="italic font-CoFoMedium">Pago</span> a colaboradores.
                </li>
                <li class="text-gray-600 text-lg">
                  <span class="text-blue-600 font-semibold">⭢</span>
                  <span class="italic font-CoFoMedium">Disperción</span> de fondos.
                </li>
                <li class="text-gray-600 text-lg">
                  <span class="text-blue-600 font-semibold">⭢</span>
                  <span class="italic font-CoFoMedium">Monitoreo</span> de gastos
                  y movimientos.
                </li>
                <li class="text-gray-600 text-lg">
                  <span class="text-blue-600 font-semibold">⭢</span>
                  <span class="italic font-CoFoMedium">Reporte</span> financieros
                  mensuales.
                </li>
                <li class="text-gray-600 text-lg">
                  <span class="text-blue-600 font-semibold">⭢</span>
                  <span class="italic font-CoFoMedium">Rebulneración </span>
                  de dinero en cuenta.
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div
          class="flex justify-center items-center flex-row gap-16 w-4/5 max-[1280px]:w-full max-[1280px]:flex-col-reverse views"
        >
          <div
            class="relative bg-white w-full rounded-3xl h-full px-10 max-[450px]:px-5 py-5 flex justify-center items-center flex-col gap-5 max-[1280px]:w-2/4 max-[800px]:w-[90vw]"
